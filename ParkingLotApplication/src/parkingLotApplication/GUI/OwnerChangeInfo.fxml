<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="450.0" style="-fx-background-color: silver;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parkingLotApplication.OwnerChangeInfoController">
   <children>
      <Button layoutX="200.0" layoutY="350.0" mnemonicParsing="false" onAction="#changeInfoBtn" prefHeight="30.0" prefWidth="50.0" text="변경" />
      <VBox layoutX="125.0" layoutY="43.0">
         <children>
            <Label prefHeight="30.0" prefWidth="200.0" text="Change_Password" />
            <TextField fx:id="passwordField" prefHeight="30.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="30.0" prefWidth="200.0" text="Change_Name" />
            <TextField fx:id="nameField" prefHeight="30.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="30.0" prefWidth="200.0" text="Change_Age" />
            <TextField fx:id="ageField" prefHeight="30.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label prefHeight="30.0" prefWidth="200.0" text="Change_AccountNumber" />
            <TextField fx:id="accountNumField" prefHeight="30.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
