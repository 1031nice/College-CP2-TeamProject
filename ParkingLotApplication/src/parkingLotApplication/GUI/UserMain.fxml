<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parkingLotApplication.GUI.UserMainController">
   <children>
      <AnchorPane layoutX="20.0" layoutY="70.0" prefHeight="400.0" prefWidth="450.0" style="-fx-background-color: silver;">
         <children>
            <HBox prefHeight="75.0" prefWidth="450.0" />
            <HBox layoutY="322.0" prefHeight="78.0" prefWidth="450.0" />
         </children></AnchorPane>
      <HBox layoutX="20.0" layoutY="20.0" prefHeight="30.0" prefWidth="350.0">
         <children>
            <Label fx:id="userName" maxWidth="1.7976931348623157E308" text="ooo" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label maxWidth="1.7976931348623157E308" text="고객님환영합니다." HBox.hgrow="ALWAYS">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox layoutX="485.0" layoutY="70.0" prefHeight="400.0" prefWidth="100.0">
         <children>
            <Button fx:id="reservationButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#reservationAction" prefHeight="50.0" text="예약" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="returnButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#returnAction" prefHeight="50.0" text="반납">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="paymentButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#paymentAction" prefHeight="50.0" text="결제">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="changeInfoButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#changeInfoAction" prefHeight="50.0" text="정보변경">
