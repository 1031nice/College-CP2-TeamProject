            </Button>
            <Button fx:id="logoutButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#logoutAction" prefHeight="50.0" text="돌아가기">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#exitAction" prefHeight="50.0" text="종료" VBox.vgrow="ALWAYS" />
